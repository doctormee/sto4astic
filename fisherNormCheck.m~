function [ res ] = fisherNormCheck( ksi1, ksi2, alphaVal )
    nKsi1 = length(ksi1);
    m = length(ksi2);
%     expE1 = mean(ksi1);
%     expE2 = mean(ksi2);
%     s1 = sum((ksi1 - expE1).^2)/(n - 1).^2;
%     s2 = sum((ksi2 - expE2).^2)/(m - 1).^2;
    s1 = sum(ksi1.^2) ./ n - (sum(ksi1) ./ n).^2;
    s2 = sum(ksi2.^2) ./ m - (sum(ksi2) ./ m).^2;
    f = s1 ./ s2;
    fBound = finv(alphaVal, n - 1, m - 1);
    if (abs(f) < fBound)
        res = true;
    else
        res = false;
    end
end

